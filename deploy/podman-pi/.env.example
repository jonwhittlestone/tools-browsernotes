# Authentication password for the web interface
AUTH_PASSWORD=change-me

# Secret key for signing session cookies (generate with: python -c "import secrets; print(secrets.token_hex(32))")
SECRET_KEY=change-me

# Dropbox app key (same as the Chrome extension)
DROPBOX_APP_KEY=

# Dropbox OAuth redirect URI
DROPBOX_REDIRECT_URI=https://howapped.zapto.org/browsernotes/api/dropbox/callback

# Server port
PORT=3004
